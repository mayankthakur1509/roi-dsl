{
  "agent_type": "roi_qualification_bot",
  "persona": {
    "name": "ROI Qualification Assistant",
    "role": "value_discovery",
    "tone": "professional_empathetic",
    "target_persona": {
      "role": "Developer",
      "description": "Professional Web Developer Test15"
    }
  },
  "goals": [
    {
      "goal_id": "FastWebsites",
      "description": "Build fast, responsive websites that load instantly",
      "type": "value_gain"
    },
    {
      "goal_id": "ModernUI",
      "description": "Create modern, mobile-friendly UI/UX",
      "type": "value_gain"
    },
    {
      "goal_id": "HighPerformance",
      "description": "Optimize websites for speed and performance",
      "type": "value_gain"
    }
  ],
  "qualification_logic": {
    "questions": [
      {
        "metric": "SpeedImprovement",
        "question": "On a scale of 0-100, how would you rate your current SpeedImprovement?",
        "threshold": 0.6,
        "weight": 1.0
      },
      {
        "metric": "UIDeliveryScore",
        "question": "On a scale of 0-100, how would you rate your current UIDeliveryScore?",
        "threshold": 0.9,
        "weight": 1.0
      },
      {
        "metric": "PerfBoost",
        "question": "On a scale of 0-100, how would you rate your current PerfBoost?",
        "threshold": 0.5,
        "weight": 1.0
      },
      {
        "metric": "DeliveredWebsites",
        "question": "On a scale of 0-100, how would you rate your current DeliveredWebsites?",
        "threshold": 40.0,
        "weight": 1.0
      },
      {
        "metric": "ExpertiseLevel",
        "question": "On a scale of 0-100, how would you rate your current ExpertiseLevel?",
        "threshold": 0.95,
        "weight": 1.0
      }
    ],
    "scoring": {
      "high_priority_threshold": 0.6,
      "medium_priority_threshold": 0.4,
      "low_priority_threshold": 0.2
    }
  },
  "conversation_flow": [
    {
      "step": 1,
      "type": "greeting",
      "message": "Hi! I help companies understand their ROI opportunities. Can I ask you a few quick questions?"
    },
    {
      "step": 2,
      "type": "pain_discovery",
      "message": "I noticed you're interested in Build fast, responsive websites that load instantly. What's driving this need?"
    },
    {
      "step": 3,
      "type": "qualification",
      "message": "Let me ask a few diagnostic questions to understand your situation better..."
    },
    {
      "step": 4,
      "type": "metric_question",
      "metric": "SpeedImprovement",
      "message": "How would you describe your current SpeedImprovement?"
    },
    {
      "step": 5,
      "type": "metric_question",
      "metric": "UIDeliveryScore",
      "message": "How would you describe your current UIDeliveryScore?"
    },
    {
      "step": 6,
      "type": "metric_question",
      "metric": "PerfBoost",
      "message": "How would you describe your current PerfBoost?"
    },
    {
      "step": 7,
      "type": "metric_question",
      "metric": "DeliveredWebsites",
      "message": "How would you describe your current DeliveredWebsites?"
    },
    {
      "step": 8,
      "type": "metric_question",
      "metric": "ExpertiseLevel",
      "message": "How would you describe your current ExpertiseLevel?"
    },
    {
      "step": 9,
      "type": "recommendation",
      "message": "Based on your answers, I can see some opportunities. Would you like to schedule a detailed ROI assessment?"
    }
  ],
  "automation_rules": [],
  "escalation_criteria": {
    "immediate_escalation": [
      {
        "condition": "UIDeliveryScore > 0.6",
        "reason": "metric_threshold_exceeded"
      },
      {
        "condition": "DeliveredWebsites > 0.6",
        "reason": "metric_threshold_exceeded"
      },
      {
        "condition": "ExpertiseLevel > 0.6",
        "reason": "metric_threshold_exceeded"
      }
    ],
    "scheduled_followup": [],
    "nurture_sequence": []
  },
  "metrics_tracking": {
    "SpeedImprovement": 0.6,
    "UIDeliveryScore": 0.9,
    "PerfBoost": 0.5,
    "DeliveredWebsites": 40.0,
    "ExpertiseLevel": 0.95
  }
}